<label for="model" class="text-montserrat">Модели</label>
<select id="model" class="select-filter text-montserrat">
    <option class="text-montserrat" value="">Все модели</option>
    {% for model in models %}
        <option class="text-montserrat" value="{{ model['id'] }}">{{ model['name'] }}</option>
    {% endfor %}
</select>